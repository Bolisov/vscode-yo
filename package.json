{
  "name": "yo",
  "version": "0.2.1",
  "description": "Scaffold projects using Yeoman.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/SamVerschueren/vscode-yo"
  },
  "author": {
    "name": "Sam Verschueren",
    "email": "sam.verschueren@gmail.com",
    "url": "github.com/SamVerschueren"
  },
  "publisher": "samverschueren",
  "icon": "media/icon.png",
  "engines": {
    "vscode": "0.10.x"
  },
  "activationEvents": [
    "onCommand:yo"
  ],
  "main": "./out/extension",
  "contributes": {
    "commands": [
      {
        "command": "yo",
        "title": "yo"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "node ./node_modules/vscode/bin/compile",
    "compile": "node ./node_modules/vscode/bin/compile -watch -p ./"
  },
  "devDependencies": {
    "vscode": "*"
  },
  "dependencies": {
    "error-ex": "^1.3.0",
    "figures": "^1.4.0",
    "find-up": "^1.0.0",
    "fix-path": "^1.1.0",
    "semver": "^5.0.3",
    "yeoman-environment": "^1.3.0"
  }
}
