{
  "name": "vscode-yo",
  "description": "Scaffold projects using Yeoman.",
  "version": "0.0.1",
  "publisher": "samverschueren",
  "engines": {
    "vscode": "*"
  },
  "activationEvents": [
    "command:yo"
  ],
  "main": "./out/extension",
  "contributes": {
    "commands": [
      {
        "command": "yo",
        "title": "Yo"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "node ./node_modules/vscode/bin/compile",
    "compile": "node ./node_modules/vscode/bin/compile -watch -p ./"
  },
  "devDependencies": {
    "vscode": "*"
  },
  "dependencies": {
    "figures": "^1.4.0",
    "findup-sync": "^0.3.0",
    "fix-path": "^1.1.0",
    "lodash": "^3.10.1",
    "semver": "^5.0.3",
    "yeoman-environment": "^1.3.0"
  }
}
